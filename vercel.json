  "framework": "nextjs",
  "regions": ["fra1"],
  "functions": {
    "src/app/**/*.ts": {
      "runtime": "nodejs18.x"
    },
    "src/app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_SUPABASE_URL": "@supabase-url",
